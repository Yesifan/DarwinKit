{
	"name": "darkit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.11.3",
		"@inlang/paraglide-sveltekit": "^0.11.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.4",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^9.6.0",
		"@types/prismjs": "^1.26.5",
		"autoprefixer": "^10.4.19",
		"bits-ui": "1.0.0-next.66",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.3.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.424.0",
		"mdsvex": "^0.12.3",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.4",
		"svelte": "^5.0.5",
		"svelte-check": "^3.6.0",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.4",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@neoconfetti/svelte": "^2.2.1",
		"@xyflow/svelte": "^0.1.23",
		"codejar": "^4.2.0",
		"echarts": "^5.5.1",
		"front-matter": "^4.0.2",
		"mode-watcher": "^0.4.1",
		"prismjs": "^1.29.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1"
	}
}