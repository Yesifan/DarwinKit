		"mode-watcher": "^0.4.1",
		"prismjs": "^1.29.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.11.3",
		"@inlang/paraglide-sveltekit": "^0.11.5",
